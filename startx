feh --bg-fill /home/pj/.dotfiles/backgrounds/mystical-night-in-town.jpg

xrdb -merge $HOME/.Xresources
xmodmap ~/.Xmodmap
setxkbmap -model abnt2 -layout br

xinput set-prop "ELAN0B00:00 04F3:3261 Touchpad" "libinput Tapping Enabled" 1
xinput set-prop "ELAN0B00:00 04F3:3261 Touchpad" "libinput Natural Scrolling Enabled" 1


$HOME/.dotfiles/bar/bar >/dev/null 2>&1 &
picom -b --config $HOME/.dotfiles/picom.conf >/dev/null 2>&1 &
dbus-run-session dwm
